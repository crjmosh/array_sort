source 'https://rubygems.org'
# optional - but best practice
ruby '2.3.1'

# gem 'whatever the name of the gem is'
# semver - '2.0.1' - 2 is major, 0 is minor, 1 is patch
# strict pin - '2.1.0' - THIS NEVER UPDATES
# ~> pin - '~> '2.1.0' - UPDATE to MINOR and PATCH version
gem 'pry', '~> 0.10.4'
gem 'babbler', '~> 2.0.0'

# unpinned gem
# ALWAYS going to update to the most recent version
# NOT RECOMMENDED
# gem 'pry'